+++ b/frontend/src/index.css	(date 1764804103429)
@@ -140,3 +140,40 @@
   }
 
 }
+
+/* Alerts como toasts flutuantes no topo da tela quando .toastify-alerts está presente no body */
+body.toastify-alerts .alert {
+  position: fixed;
+  top: 16px;
+  left: 50%;
+  transform: translateX(-50%);
+  width: min(360px, calc(100vw - 32px));
+  z-index: 9999;
+  margin: 0;
+  box-shadow: 0 12px 30px rgba(0, 0, 0, 0.18);
+  animation: toast-in 180ms ease-out, toast-out 180ms ease-in 4s forwards;
+  text-align: center;
+  justify-content: center;
+}
+
+body.toastify-alerts .alert + .alert {
+  margin-top: 12px;
+}
+
+@keyframes toast-in {
+  from {
+    opacity: 0;
+    translate: 0 -8px;
+  }
+  to {
+    opacity: 1;
+    translate: 0 0;
+  }
+}
+
+@keyframes toast-out {
+  to {
+    opacity: 0;
+    translate: 0 -6px;
+  }
+}
